{
	"name": "quick",
	"displayName": "Quick",
	"description": "Quick start project for multiple types",
	"version": "0.0.1",
	"publisher": "sun-xm",
	"engines": {
		"vscode": "^1.52.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:quick.cmakeConsole",
		"onCommand:quick.cmakeQtWidgets",
		"onCommand:quick.cmakeW32View",
		"onCommand:quick.electronApp",
		"onCommand:quick.electronMod",
		"onCommand:quick.openUiFile"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "quick.cmakeConsole",
				"title": "Quick: Start cmake console application"
			},
			{
				"command": "quick.cmakeQtWidgets",
				"title": "Quick: Start cmake Qt Widgets application"
			},
			{
				"command": "quick.cmakeW32View",
				"title": "Quick: Start cmake Win32 View application"
			},
			{
				"command": "quick.electronApp",
				"title": "Quick: Start Electron application"
			},
			{
				"command": "quick.electronMod",
				"title": "Quick: Add Electron module"
			},
			{
				"command": "quick.openUiFile",
				"title": "Open in Qt Designer"
			}
		],
		"menus": {
			"explorer/context": [
				{
					"command": "quick.openUiFile",
					"group": "1_modification",
					"when": "resourceExtname == .ui"
				}
			]
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js",
		"package": "vsce package"
	},
	"devDependencies": {
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.0.4",
		"@types/node": "^12.11.7",
		"@types/vscode": "^1.52.0",
		"@typescript-eslint/eslint-plugin": "^4.9.0",
		"@typescript-eslint/parser": "^4.9.0",
		"eslint": "^7.15.0",
		"glob": "^7.1.6",
		"mocha": "^9.2.1",
		"typescript": "^4.1.2",
		"vsce": "^2.6.7",
		"vscode-test": "^1.4.1"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/sun-xm/Quick.git"
	}
}
